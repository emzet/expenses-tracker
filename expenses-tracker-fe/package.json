{
  "name": "@expenses-tracker/client",
  "description": "Expenses Tracker [client]",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Marek Zavacky",
    "email": "zavacky.m@gmail.com"
  },
  "engines": {
    "npm": "10.8.1",
    "node": "v20.14.0"
  },
  "packageManager": "npm@10.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/emzet/expenses-tracker"
  },
  "scripts": {
    "update": "ncu --interactive --format group --root",
    "ng": "ng",
    "ngxs": "ngxs",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build --configuration development",
    "build:prod": "ng build",
    "lint:ls": "ls-lint",
    "lint:pkg": "npmPkgJsonLint ./package.json --config .npmpackagejsonlintrc",
    "lint:i18n": "transloco-keys-manager find --emit-error-on-extra-keys",
    "lint:html": "htmlhint \"src/**/*.html\" --config .htmlhintrc.json",
    "lint:lf": "lockfile-lint",
    "lint:lf:ci": "lockfile-lint --format plain",
    "lint:md": "markdownlint *.md",
    "lint:md:fix": "markdownlint *.md --fix",
    "lint:style": "stylelint **/**.scss",
    "lint:style:fix": "stylelint **/**.scss --fix",
    "lint:style:ci": "mkdirp reports && stylelint **/**.scss --formatter json > reports/stylelint-report.json",
    "lint": "run-p --print-label lint:ls lint:pkg lint:i18n lint:html lint:lf lint:md lint:style",
    "lint:fix": "run-p --print-label lint:ls lint:pkg lint:i18n lint:html lint:lf lint:md:fix lint:style:fix",
    "i18n:find": "transloco-keys-manager find",
    "i18n:extract": "transloco-keys-manager extract",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "rimraf docs && compodoc --disableRoutesGraph",
    "docs:serve": "compodoc --disableRoutesGraph --serve --open",
    "analyzer": "vite-bundle-visualizer dist/expenses-tracker/stats.json",
    "svgo": "svgo --folder src/assets/images --recursive"
  },
  "dependencies": {
    "@angular/animations": "18.0.3",
    "@angular/cdk": "18.0.3",
    "@angular/common": "18.0.3",
    "@angular/compiler": "18.0.3",
    "@angular/core": "18.0.3",
    "@angular/forms": "18.0.3",
    "@angular/platform-browser": "18.0.3",
    "@angular/platform-browser-dynamic": "18.0.3",
    "@angular/router": "18.0.3",
    "@jsverse/transloco": "7.4.2",
    "@maskito/angular": "2.4.0",
    "@maskito/core": "2.4.0",
    "@maskito/kit": "2.4.0",
    "@nebular/auth": "13.0.0",
    "@nebular/date-fns": "13.0.0",
    "@nebular/eva-icons": "13.0.0",
    "@nebular/theme": "13.0.0",
    "@ngxs/router-plugin": "18.0.0",
    "@ngxs/store": "18.0.0",
    "chart.js": "4.4.3",
    "date-fns": "2.30.0",
    "eva-icons": "1.1.3",
    "ng2-charts": "6.0.1",
    "ngx-cookie-service": "18.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.4",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "18.0.4",
    "@angular/compiler-cli": "18.0.3",
    "@compodoc/compodoc": "1.1.25",
    "@jsverse/transloco-keys-manager": "4.2.2",
    "@ls-lint/ls-lint": "2.2.3",
    "@ngxs/cli": "3.0.0",
    "@ngxs/devtools-plugin": "18.0.0",
    "@schematics/angular": "18.0.4",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "eslint": "8.57.0",
    "htmlhint": "1.1.4",
    "lockfile-lint": "4.14.0",
    "ls-lint": "0.1.2",
    "markdownlint-cli": "0.41.0",
    "npm-check-updates": "16.14.20",
    "npm-package-json-lint": "8.0.0",
    "npm-package-json-lint-config-default": "7.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.2",
    "rimraf": "5.0.7",
    "stylelint": "16.6.1",
    "stylelint-config-standard-scss": "13.1.0",
    "svgo": "3.3.2",
    "typescript": "5.4.5",
    "vite-bundle-visualizer": "1.2.1"
  }
}
